<p id="notice"><%= notice %></p>

<h1>Trandetails</h1>

<table>
  <thead>
    <tr>
      <th>Transaction</th>
      <th>Product</th>
      <th>Saleprice</th>
      <th>Prodqty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trandetails.each do |trandetail| %>
      <tr>
        <td><%= trandetail.transaction_id %></td>
        <td><%= trandetail.product_id %></td>
        <td><%= trandetail.saleprice %></td>
        <td><%= trandetail.prodqty %></td>
        <td><%= link_to 'Show', trandetail %></td>
        <td><%= link_to 'Edit', edit_trandetail_path(trandetail) %></td>
        <td><%= link_to 'Destroy', trandetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trandetail', new_trandetail_path %>
